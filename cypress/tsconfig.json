  "include": ["./**/*", "/support/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "isolatedModules": false,
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["../src/*"]
    }
  }
}
